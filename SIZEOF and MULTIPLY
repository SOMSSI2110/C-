#include <iostream>
using namespace std;
int main() 
{    
    cout << "Size of char: " << sizeof(char);
    cout << "Size of int: " << sizeof(int);
    cout << "Size of float: " << sizeof(float);
    cout << "Size of double: " << sizeof(double);

    return 0;
}


#include <iostream>
using namespace std;
int main()
{
	float a,b,c;
	cout<<"Enter two numbers";
	cin>>a>>b;
	c=a*b;
	cout<<"Product"<<c;
	return 0;
}
